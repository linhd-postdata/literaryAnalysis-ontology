###  http://postdata.linhd.uned.es/ontology/POSTDATA-literaryAnalysis.owl/vc-figureCategory
<http://postdata.linhd.uned.es/ontology/POSTDATA-literaryAnalysis.owl/vc-figureCategory> rdf:type rdfs:Datatype .


###  http://postdata.linhd.uned.es/ontology/POSTDATA-literaryAnalysis.owl/dataType#vc-typeOfIntertextuality
<http://postdata.linhd.uned.es/ontology/POSTDATA-literaryAnalysis.owl/dataType#vc-typeOfIntertextuality> rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#figureCategory
:figureCategory rdf:type owl:ObjectProperty ;
                rdfs:domain :FigureOfSpeech ;
                rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                             owl:hasValue <http://postdata.linhd.uned.es/kos/figureCategory>
                           ] ;
                rdfs:comment "Categorisation of the item at hand."@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                rdfs:label "figureCategory"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#hasAcrostic
:hasAcrostic rdf:type owl:ObjectProperty ;
             owl:inverseOf :isContainedIn ;
             rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
             rdfs:range :Acrostic ;
             rdfs:comment "This property points to the Acrostic contained in the current instance of the Redaction"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
             rdfs:label "hasAcrostic"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#hasContemporaryRelation
:hasContemporaryRelation rdf:type owl:ObjectProperty ;
                         owl:inverseOf :isRelatedContemporaneouslyWith ;
                         rdfs:domain :Intertextuality ;
                         rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
                         rdfs:comment "This property points to the work under analysis (Redaction) with which the current cultural product, that is contemporary to it, has some kind of intertextual relationship"@en ;
                         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                         rdfs:label "hasContemporaryRelation"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#hasDerivedWork
:hasDerivedWork rdf:type owl:ObjectProperty ;
                owl:inverseOf :isDerivedFrom ;
                rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
                rdfs:range :Intertextuality ;
                rdfs:comment "This property points to a cultural product (Intertextuality) which was based on the work under analysis"@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                rdfs:label "hasDerivedWork"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#hasRelationsWith
:hasRelationsWith rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isRelatedWith ;
                  rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
                  rdfs:range :Intertextuality ;
                  rdfs:comment "This property points to the text under analysis (Redaction) with which the current cultural product has a generic intertextual relationship"@en ;
                  rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                  rdfs:label "hasRelationsWith"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#hasSmallerUnits
:hasSmallerUnits rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isSmallerUnitOf ;
                 rdfs:domain :FigureOfSpeech ;
                 rdfs:range :FigureOfSpeech ;
                 rdfs:comment "The current instance of Figure of Speech can be decompounded in smaller units for a more granular anlysis (e.g. a kenning made of other kennings)"@en ;
                 rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                 rdfs:label "hasSmallerUnits"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#isContainedIn
:isContainedIn rdf:type owl:ObjectProperty ;
               rdfs:domain :Acrostic ;
               rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
               rdfs:comment "This property points to the Redaction (version of the Opus) in which the Acrostic is present"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
               rdfs:label "isContainedIn"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#isDerivedFrom
:isDerivedFrom rdf:type owl:ObjectProperty ;
               rdfs:domain :Intertextuality ;
               rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
               rdfs:comment "This property points to the text under analysis (Redaction) which has been a source for the current cultural product"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
               rdfs:label "isDerivedFrom"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#isPresentIn
:isPresentIn rdf:type owl:ObjectProperty ;
             rdfs:domain :FigureOfSpeech ;
             rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-structuralElements#Line> ;
             rdfs:comment "This property points to the lines (instance of Line) or word (instance of Word) which contains the current item under analysis"@en ;
             rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
             rdfs:label "isPresentIn"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#isReferencedIn
:isReferencedIn rdf:type owl:ObjectProperty ;
                owl:inverseOf :refersTo ;
                rdfs:range :Acrostic ;
                rdfs:comment "This property points to the Acrostic in which the current instance of Person of Place is referenced"@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                rdfs:label "isReferencedIn"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#isRelatedContemporaneouslyWith
:isRelatedContemporaneouslyWith rdf:type owl:ObjectProperty ;
                                rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
                                rdfs:range :Intertextuality ;
                                rdfs:comment "This property points to a cultural product (Intertextuality) which is contemporary to the work under analysis and some kind of intertextual relationship is detected"@en ;
                                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                                rdfs:label "isRelatedContemporaneouslyWith"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#isRelatedWith
:isRelatedWith rdf:type owl:ObjectProperty ;
               rdfs:domain :Intertextuality ;
               rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
               rdfs:comment "This property points to a cultural product (Intertextuality) which has a generic intertextual relationship with the work under analysis"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
               rdfs:label "isRelatedWith"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#isSmallerUnitOf
:isSmallerUnitOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :FigureOfSpeech ;
                 rdfs:range :FigureOfSpeech ;
                 rdfs:comment "The current instance of Figure of Speech is a smaller unit of another instance of Figure Of Speech"@en ;
                 rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                 rdfs:label "isSmallerUnit"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#isSource
:isSource rdf:type owl:ObjectProperty ;
          owl:inverseOf :usesAsSource ;
          rdfs:domain :Intertextuality ;
          rdfs:range <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
          rdfs:comment "This property points to the text under analysis (Redaction) from which the current cultural product has derived"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
          rdfs:label "isSource"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#refersTo
:refersTo rdf:type owl:ObjectProperty ;
          rdfs:domain :Acrostic ;
          rdfs:comment "This property points to a Person or Place that is referred in the Acrostic"@en ;
          rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
          rdfs:label "refersTo"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#rethoricalOperation
:rethoricalOperation rdf:type owl:ObjectProperty ;
                     rdfs:domain :FigureOfSpeech ;
                     rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                  owl:hasValue <http://postdata.linhd.uned.es/kos/rethoricalOperation>
                                ] ;
                     rdfs:comment "Classification categories of speech figures in relation to rhetorical operations: addition, omission, transposition and transmutation"@en ;
                     rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                     rdfs:label "rethoricalOperation"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#typeOfIntertextuality
:typeOfIntertextuality rdf:type owl:ObjectProperty ;
                       rdfs:domain :Intertextuality ;
                       rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                    owl:hasValue <http://postdata.linhd.uned.es/kos/typeOfIntertextuality>
                                  ] ;
                       rdfs:comment "Categorisation of the work under analysis depending on the relationship between this work and other texts. E.g. “contrafactum”, “de seguir”, etc."@en ;
                       rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                       rdfs:label "typeOfIntertextuality"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#usesAsSource
:usesAsSource rdf:type owl:ObjectProperty ;
              rdfs:domain <http://postdata.linhd.uned.es/ontology/postdata-core#Redaction> ;
              rdfs:range :Intertextuality ;
              rdfs:comment "This property points to a cultural product (Intertextuality) which was a source for the work under analysis"@en ;
              rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
              rdfs:label "usesAsSource"@en .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:domain owl:Thing .


