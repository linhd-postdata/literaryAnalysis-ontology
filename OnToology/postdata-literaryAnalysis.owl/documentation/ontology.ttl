###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#content
:content rdf:type owl:DatatypeProperty ;
         rdfs:comment """Full content of the literary resource. For example, the content of the  acrostic is  the letters that compose it.
En la figura de la palabra, el contenido es una palabra o frase que se utiliza en la figura y que posee un significado separado de su definición literal"""@en ;
         rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
         rdfs:label "content"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#figureMeaning
:figureMeaning rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :FigureOfSpeech ;
               rdfs:range xsd:string ;
               <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
               <http://purl.org/dc/elements/1.1/date> "2018-06-16T18:39:21Z"^^xsd:dateTime ;
               rdfs:comment "Meaning of the figure"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
               rdfs:label "figureMeaning"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#figureReferent
:figureReferent rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:domain :FigureOfSpeech ;
                rdfs:range xsd:string ;
                <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
                <http://purl.org/dc/elements/1.1/date> "2018-06-16T18:39:21Z"^^xsd:dateTime ;
                rdfs:comment "Unspoken name which is equivalent to the figure"@en ;
                rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                rdfs:label "figureReferent"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#isAbecedarius
:isAbecedarius rdf:type owl:DatatypeProperty ;
               rdfs:domain :Acrostic ;
               rdfs:range xsd:boolean ;
               rdfs:comment "Whether the composition is an abecedarius, that is an acrostic in which the first letter of every word, strophe or verse follows the order of the alphabet"@en ;
               rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
               rdfs:label "isAbecedarius"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#locus
:locus rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty ;
       rdfs:domain :Intertextuality ;
       rdfs:range xsd:string ;
       <http://purl.org/dc/elements/1.1/creator> "mluisadiez" ;
       rdfs:comment "The particular position of the item at hand in relation to the PrimarySource or in a text"@en ;
       rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements"@en ;
       rdfs:label "locus"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#reference
:reference rdf:type owl:DatatypeProperty ;
           owl:equivalentProperty <http://purl.org/dc/terms/bibliographicCitation> ;
           rdfs:domain :Intertextuality ;
           rdfs:range xsd:string ;
           rdfs:comment "A non-structured mention of the textual source with which there is intertextual relationships"@en ;
           rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
           rdfs:label "reference"@en .


###  http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements#referenceLocation
:referenceLocation rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Intertextuality ;
                   rdfs:range xsd:string ;
                   rdfs:comment "The location of the relevant piece of information in that particular textual source with which there is intertextual relationships"@en ;
                   rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
                   rdfs:label "referenceLocation"@en .


